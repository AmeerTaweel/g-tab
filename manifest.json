{
    "name": "gTab",
    "version": "1.0",
    "description": "gTab is a tab management extinction for Google Chrome, built with Vue.js.",
    "permissions": ["storage"],
    "background": {
        "scripts": ["./js/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "./index.html",
        "default_icon": {
            "16": "./icons/ex_ico_16.png",
            "48": "./icons/ex_ico_48.png",
            "32": "./icons/ex_ico_32.png",
            "128": "./icons/ex_ico_128.png"
        }
    },
    "icons": {
        "48": "./icons/ex_ico_48.png",
        "16": "./icons/ex_ico_16.png",
        "32": "./icons/ex_ico_32.png",
        "128": "./icons/ex_ico_128.png"
    },
    "manifest_version": 2
}